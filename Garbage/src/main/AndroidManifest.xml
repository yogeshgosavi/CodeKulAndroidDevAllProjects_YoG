<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codekul.shopinginteractkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".JeansActivity"
            android:label="Jeans"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ShirtsActivity"
            android:label="Shirts"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".JacketsActivity"
            android:label="Jackets"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".CartActivity"
            android:label="cart Information"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>